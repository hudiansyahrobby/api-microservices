{
    "name": "backend",
    "version": "1.0.0",
    "description": "project",
    "main": "index.js",
    "scripts": {
        "start": "cross-env NODE_ENV=production npm run build && pm2 start ./dist/index.js",
        "dev": "cross-env NODE_ENV=development nodemon index.ts",
        "test": "cross-env NODE_ENV=test jest --detectOpenHandles",
        "migrate": "npx sequelize-cli db:migrate",
        "migrate:reset": "npx sequelize-cli db:migrate:undo:all && npm run migrate",
        "seed": "npx sequelize-cli db:seed:all --debug",
        "seed-test": "cross-env NODE_ENV=test npx sequelize-cli db:seed:all",
        "seed-reset": "npx sequelize-cli db:seed:undo:all && npm run seed",
        "build": "tsc --project ./",
        "lint": "eslint '*/**/*.{js,ts,tsx}' --quiet --fix"
    },
    "keywords": [],
    "author": "",
    "license": "ISC",
    "dependencies": {
        "compression": "^1.7.4",
        "cors": "^2.8.5",
        "dotenv": "^8.2.0",
        "express": "^4.17.1",
        "firebase": "^8.2.7",
        "firebase-admin": "^9.5.0",
        "helmet": "^4.4.1",
        "joi": "^17.3.0",
        "morgan": "^1.10.0",
        "pg": "^8.6.0",
        "pg-hstore": "^2.3.3",
        "pm2": "^4.5.4",
        "reflect-metadata": "^0.1.13",
        "sequelize": "^6.6.2",
        "sequelize-typescript": "^2.1.0",
        "swagger-jsdoc": "^6.0.6",
        "swagger-ui-express": "^4.1.6",
        "winston": "^3.3.3"
    },
    "devDependencies": {
        "@babel/plugin-proposal-decorators": "^7.12.13",
        "@babel/preset-env": "^7.12.17",
        "@babel/preset-typescript": "^7.12.17",
        "@types/bcryptjs": "^2.4.2",
        "@types/compression": "^1.7.0",
        "@types/cors": "^2.8.9",
        "@types/express": "^4.17.11",
        "@types/jest": "^26.0.20",
        "@types/morgan": "^1.9.2",
        "@types/node": "^14.14.25",
        "@types/supertest": "^2.0.10",
        "@types/swagger-jsdoc": "^6.0.0",
        "@types/swagger-ui-express": "^4.1.2",
        "@types/validator": "^13.1.3",
        "@typescript-eslint/eslint-plugin": "^4.15.2",
        "@typescript-eslint/parser": "^4.15.2",
        "cross-env": "^7.0.3",
        "eslint": "^7.20.0",
        "eslint-config-prettier": "^8.0.0",
        "eslint-plugin-prettier": "^3.3.1",
        "jest": "^26.6.3",
        "nodemon": "^2.0.7",
        "prettier": "^2.2.1",
        "sequelize-cli": "^6.2.0",
        "supertest": "^6.1.3",
        "ts-node": "^9.1.1",
        "typescript": "^4.1.3"
    }
}
